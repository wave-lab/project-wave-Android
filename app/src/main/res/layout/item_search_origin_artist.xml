<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        android:id="@+id/bt_rv_item_search_origin_artist"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_marginVertical="6dp"
        android:background="#000000">
    <ImageView
            android:id="@+id/iv_search_origin_artist_item_image"
            android:src="@drawable/img_dart"
            android:layout_width="82dp"
            android:layout_height="82dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"/>
    <RelativeLayout
            android:layout_toRightOf="@+id/iv_search_origin_artist_item_image"
            android:layout_marginRight="24dp"
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:orientation="horizontal">
            <TextView
                    android:id="@+id/tv_search_origin_artist_item_title"
                    android:textColor="#ffffff"
                    android:textSize="16sp"
                    android:text="Day6(데이식스)"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>