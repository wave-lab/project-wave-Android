            android:layout_width="match_parent"
            android:layout_height="16dp"
            android:layout_marginTop="20dp"
            >
            <ImageView
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/btn_login_beforenav"
                android:layout_marginLeft="16dp"
                android:scaleType="fitXY"
                />

        </RelativeLayout>

        <RelativeLayout
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView
                    android:id="@+id/id_mood_act_mood"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="어떤 분위기의 커버곡 인가요?"
                    android:textSize="18dp"
                    android:layout_marginTop="14dp"
                    android:fontFamily="@font/noto_sans_kr_bold_hestia"
                    android:textColor="#ffffff"
                    android:includeFontPadding="false"/>
            <View
                    android:layout_below="@id/id_mood_act_mood"
                    android:layout_alignStart="@id/id_mood_act_mood"
                    android:layout_width="50dp"
                    android:layout_height="3dp"
                    android:background="#00b6de"
                    android:layout_marginStart="36dp"
            />
        </RelativeLayout>



    </LinearLayout>
