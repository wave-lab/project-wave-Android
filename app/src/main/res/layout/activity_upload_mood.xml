            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="신나는"
            android:textSize="16sp"
            android:textColor="#ffffff"
            android:layout_centerInParent="true"
            android:fontFamily="@font/noto_sans_kr_bold_hestia"
